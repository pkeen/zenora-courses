{
	"name": "zenora-app-nextjs",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"push:dev": "dotenv -e .env.development drizzle-kit push --url $DATABASE_URL",
