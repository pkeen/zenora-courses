		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"dotenv-cli": "^7.4.2",
		"drizzle-kit": "^0.28.0",
		"eslint": "^8",
		"eslint-config-next": "15.0.3",
		"postcss": "^8",
		"tailwindcss": "^3.4.1",
		"typescript": "^5"
